<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LogInAdminFormController">
   <children>
      <AnchorPane fx:id="LogInFormContext" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #95a5a6;">
         <children>
            <Button layoutX="300.0" layoutY="308.0" mnemonicParsing="false" onAction="#openOrderFormOnAction" prefHeight="36.0" prefWidth="106.0" style="-fx-background-color: #3498db;" text="LogIn">
               <font>
                  <Font size="19.0" />
               </font>
            </Button>
            <Label layoutX="179.0" layoutY="29.0" text="Admin Login">
               <font>
                  <Font size="34.0" />
               </font>
            </Label>
            <Label layoutX="131.0" layoutY="132.0" text="User Name:">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label layoutX="133.0" layoutY="213.0" text="Password:">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <TextField fx:id="userNameContext" layoutX="248.0" layoutY="131.0" onAction="#movePassword" />
            <PasswordField fx:id="passwordContext" layoutX="248.0" layoutY="214.0" />
            <Button fx:id="cancelBtnContext" layoutX="179.0" layoutY="308.0" mnemonicParsing="false" onAction="#backLogInForm" prefHeight="36.0" prefWidth="102.0" style="-fx-background-color: white;" text="Cancel">
               <font>
                  <Font size="19.0" />
               </font>
            </Button>
            <Label fx:id="lblAttempt" layoutX="248.0" layoutY="246.0" prefHeight="24.0" prefWidth="211.0" text="Invalid User Name Or Password" textFill="RED" visible="false" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
